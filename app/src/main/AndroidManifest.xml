<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.konstantin.maltsev.myfirstaidkit">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="false"
        android:theme="@style/Theme.MyFirstAidKit">
        <activity
            android:name=".activities.GetApiUrlActivity" />
        <activity
            android:name=".activities.EntryActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MedicineActivity"/>
        <activity
            android:name=".activities.MedicationListActivity" />
    </application>

</manifest>